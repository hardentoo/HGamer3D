abstract pass SkyBox_Pass
{
			lighting off
			depth_write off

			texture_unit
			{
				cubic_texture $texture separateUV
			}
}

material SkyBox/BlueSpace
{
	technique
	{
		pass : SkyBox_Pass
		{
			set $texture "bluespace.png"
		}
	}
}

material SkyBox/Colosseum
{
	technique
	{
		pass : SkyBox_Pass
		{
			set $texture "colosseum.jpg"
		}
	}
}

material SkyBox/SaintPetersBasilica
{
	technique
	{
		pass : SkyBox_Pass
		{
			set $texture "SaintPetersBasilica.jpg"
		}
	}
}

